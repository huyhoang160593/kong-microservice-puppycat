{
  "name": "product-services",
  "version": "1.0.0",
  "description": "Based on https://github.com/jellydn/fastify-starter with some tweak and don't need to compile schema due to using zod instead",
  "keywords": [],
  "license": "MIT",
  "author": "The 99's Puppycat",
  "main": "app.ts",
  "scripts": {
    "typecheck": "tsc -b",
    "lint": "eslint \"{src, apps, libs, test}/**/*.ts\" --fix",
    "build": "tsup",
    "build:ts": "tsup --env.NODE_ENV production",
    "dev:watch": "tsx watch src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "start": "tsx src/server.ts",
    "start:prod": "node dist/server.js",
    "start:dev": "pnpm prisma:migrate && pnpm dev:watch"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/env": "^4.2.0",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.8.0",
    "@prisma/client": "^4.14.0",
    "close-with-grace": "^1.2.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.15.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "fastify-tsconfig": "^1.0.1",
    "fastify-type-provider-zod": "^1.1.9",
    "form-data": "^4.0.0",
    "got": "^12.6.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "fast-json-stringify": "^5.7.0",
    "json-schema-to-typescript": "^12.0.0",
    "prisma": "^4.14.0",
    "sort-package-json": "^2.4.1",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "cjs"
    ],
    "target": "es2018"
  }
}
